<div class="container  mt-4">
    <div class="row ">
        <div class="col col-sm-0 col-lg-3">
            <div id="list-example" class="list-group">
                <a class="list-group-item list-group-item-action" href="#list-item-1">Tổng quan</a>
                <a class="list-group-item list-group-item-action" href="#list-item-2">ảnh</a>
                <a class="list-group-item list-group-item-action" href="#list-item-3">Tiện ích</a>
                <a class="list-group-item list-group-item-action" href="#list-item-4">Đánh giá</a>
            </div>
            <div class="mapouter"><div class="gmap_canvas">
                <iframe width="100%" height="250" id="gmap_canvas" src="https://maps.google.com/maps?q=Kh%C3%A1ch%20S%E1%BA%A1n%20Hanoi%20Central&t=&z=15&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
                <a href="https://www.embedgooglemap.net/blog/divi-discount-code-elegant-themes-coupon/">divi discount</a><br>
                <style>.mapouter{position:relative;text-align:right;height:250px;width:100%;}</style>
                <a href="https://www.embedgooglemap.net">google maps iframe generator</a>
                <style>.gmap_canvas {overflow:hidden;background:none!important;border: 8px solid rgb(248, 248, 248);height:250px;width:100%;}</style>
                </div>
            </div>
         </div>
        <div data-bs-spy="scroll" data-bs-target="#list-example" data-bs-offset="0" class="scrollspy-example col col-sm-12 col-lg-9" tabindex="0">
        <h4 id="list-item-1"></h4>
        <p>
            <h4>Tên phòng: {{detailRoom.typeRoom}}</h4>
            <p> chỉ từ VND {{detailRoom.price}} </p>
            <p>{{detailRoom.description}}</p>
        </p>
        <hr>
        <h4 id="list-item-2">ảnh</h4>
        <div class=" row">
            <div class="col col-12">
                <img src="http://localhost:3000/imgs/{{detailRoom.image}}" alt="{{detailRoom.typeRoom}}" width="100%">
            </div>
        </div>
        <hr>
        <h4 id="list-item-3">Tiện ích</h4>
        <p>{{detailRoom.extension}}</p>
        
        <hr>
        <h4 id="list-item-4">Đánh giá</h4>
        <div style="height: 500px;"></div>
        <p></p>
        </div>
    </div>
</div>